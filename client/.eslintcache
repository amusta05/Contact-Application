ntext from '../../context/auth/authContext';\nimport ContactContext from '../../context/contact/contactContext';\nconst Navbar = ({title, icon}) => {\n    const authContext = useContext(AuthContext);\n    const contactContext = useContext(ContactContext);\n    const {isAuthenticated,logout,user } = authContext;\n    const {clearContacts} = contactContext;\n    const onLogout = () => {\n        logout();\n        clearContacts();\n    }\n    const authLinks = (\n        <Fragment>\n        \n            <li>\n                <a onClick={onLogout}href=\"#!\">\n                    <i className=\"fas fa-sign-out-alt\"></i> <span className=\"hide-sm\">\n                        Logout\n                    </span>\n\n                </a>\n            </li>\n        </Fragment>\n\n    );\n    const guestLink = (\n        <Fragment>\n                <li>\n                    <Link to ='/register'>Register</Link>\n                </li>\n                <li>\n                    <Link to ='/login'>Login</Link>\n                </li>\n        </Fragment>\n    );\n    return (\n        <div className=\"navbar bg-success\">\n            <h1>\n                <i className={icon}/> {title}\n            </h1>\n            <ul>\n                {isAuthenticated ? authLinks:guestLink}\n            </ul>\n            \n        </div>\n    )\n}\n\nNavbar.propTypes = {\n    title: PropTypes.string.isRequired,\n    icon: PropTypes.string\n}\n\nNavbar.defaultProps = {\n    title: 'Contact Application',\n    icon: 'fas fa-address-book'\n \n}\n\nexport default Navbar\n","/Users/areebmustafa/Desktop/ContactApplication/client/src/components/pages/About.js",[],"/Users/areebmustafa/Desktop/ContactApplication/client/src/components/pages/Home.js",[],"/Users/areebmustafa/Desktop/ContactApplication/client/src/context/contact/ContactState.js",["113","114"],"/Users/areebmustafa/Desktop/ContactApplication/client/src/context/contact/contactReducer.js",["115","116"],"/Users/areebmustafa/Desktop/ContactAp